{
  "name": "@cqm/server",
  "version": "0.1.0",
  "description": "CQM MCPサーバー実装",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cqm-server": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "biome lint src/",
    "type-check": "tsc --noEmit",
    "start": "node dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@cqm/shared": "^0.1.0",
    "@cqm/rag": "^0.1.0",
    "ws": "^8.14.2",
    "@modelcontextprotocol/sdk": "^0.4.0",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/ws": "^8.5.10",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1"
  },
  "keywords": [
    "cqm",
    "mcp",
    "server",
    "ai"
  ],
  "author": "CQM Project",
  "license": "MIT"
}